@model Semillero_ProgramacioFinal.Models.Sesion.DocumentoEquipoViewModel

@{
    ViewBag.Title = "regristroDocumento";
    Layout = "~/Views/Shared/_LayoutSesion.cshtml";
}

@{
    if (ViewBag.error != null)
    {
        <script>
                 alert("@ViewBag.error");
        </script>
    }

}




<div class="box container">
    <div class="box-header with-border">
        <h3 class="box-title">Registro de documentos</h3>
    </div>


    <!-- /.box-header -->
    <div class=" ">
        @using (Html.BeginForm("regristroDocumento", "Sesion", FormMethod.Post))
        {


            <div class="form-horizontal">
                 
                <hr />
                @Html.ValidationSummary(true, "", new { @class = "text-danger" })


                <div class="form-group">
                    @Html.LabelFor(model => model.documento1, htmlAttributes: new { @class = "control-label col-md-2" })
                    <div class="col-md-6">
                        @Html.EditorFor(model => model.documento1, new { htmlAttributes = new { @class = "form-control" } })
                        @Html.ValidationMessageFor(model => model.documento1, "", new { @class = "text-danger" })
                    </div>
                </div>

                <div class="form-group">
                    @Html.LabelFor(model => model.documento2, htmlAttributes: new { @class = "control-label col-md-2" })
                    <div class="col-md-6">
                        @Html.EditorFor(model => model.documento2, new { htmlAttributes = new { @class = "form-control" } })
                        @Html.ValidationMessageFor(model => model.documento2, "", new { @class = "text-danger" })
                    </div>
                </div>

                <div class="form-group">
                    <div class="col-md-offset-2 col-md-10">
                        <input type="submit" value="Create" class="btn btn-default" />
                    </div>
                </div>
            </div>
        }

    </div>
</div>