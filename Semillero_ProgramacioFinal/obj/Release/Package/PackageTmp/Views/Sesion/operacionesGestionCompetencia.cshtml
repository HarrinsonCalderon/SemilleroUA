
@{
    ViewBag.Title = "operacionesGestionCompetencia";
    Layout = "~/Views/Shared/_LayoutSesion.cshtml";
}

<style>
    .b {
        margin: 0 auto;
    }

</style>

<div class="box">
    <div class="box-header">
        <h3 class="box-title">Seleccione una operacion</h3>
    </div>
    <div class="box-body   b">
        
        <a class="btn btn-app bg-aqua-active  " href="~/Sesion/editarCompetencia/@ViewBag.id">
            <i class="fa fa-edit"></i> Editar
        </a>
        <a class="btn btn-app bg-aqua-active" href="~/Sesion/gestionarSalasRegistros/@ViewBag.id">
            <i class="fa fa-cogs"></i> Gestionar
        </a>
