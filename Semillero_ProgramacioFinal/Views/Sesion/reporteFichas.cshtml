
@{
    ViewBag.Title = "reporteFichas";
    Layout = null;
    int n = (int)ViewBag.n;
}


<style>

    .flex-container {
        display: flex;
        flex-wrap: wrap;
    }

        .flex-container div {
            border: black 3px solid;
            width: 200px;
            margin: 5px;
            text-align: center;
            height: 150px;
            font-size: 40px;
        }
        img{
            max-height:200px;
            max-width:200px;
        }
    .m {
        margin-left: 50px;
        margin-right: 50px;
    }

</style>







<div class="m">
    <div class="box container ">
        

        <div class="main">
            <div class="flex-container">
                @for (int i = 1; i <= n; i++)
                {
                    <div>
                        <img src="~/img/se.jpg" />
                        <br />
                        @i
                    </div>
                }

            </div>


        </div>

    </div>


</div>