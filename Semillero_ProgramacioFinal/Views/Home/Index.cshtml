@model Semillero_ProgramacioFinal.Models.ViewModels.PlataformaViewModel
@{
    ViewBag.Title = "Index";
}
<!-- Slider -->
<div class="container">
    <div class="row">
        <div class="slider">
            <div class="img-responsive">
                <ul class="bxslider">
                    <li><img src="~/img/Fondo/logosp1.png" alt="" /></li>
                    <li><img src="~/img/Fondo/banner1.jpg" alt="" /></li>
                    <li><img src="~/img/Fondo/banner2.png" alt="" /></li>
                </ul>
            </div>
        </div>
    </div>
</div>

<!-- Descripcion -->
<div class="container">
    <div class="row">
        <div class="col-md-6 col-md-offset-3">
            <div class="text-center">
                <h2>SEMILLERO PROGRAMCIÓN UNIAMAZONIA</h2>
                <p>
                    APRENDIZAJE COLABORATIVO EN ESTRATEGIAS DE ALGORITMIA. <br> SI SIENTES PASIÓN POR LA PROGRAMACIÓN,TE INVITAMOS A QUE HAGAS PARTE DE NUESTRO SEMILLERO.
                </p>
            </div>
            <hr>
        </div>
    </div>
</div>

<!-- jueces en linea para practicar-->
<div class="container">
    <div class="row">
        <div class="box">
            <div class="col-md-4">
                <div class="wow bounceIn" data-wow-offset="0" data-wow-delay="0.4s">
