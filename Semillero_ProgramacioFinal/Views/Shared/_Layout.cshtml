<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Semillero de Programacion</title>
   
    @Styles.Render("~/Content/css");
    @Styles.Render("~/Content/bxslider");
    @Styles.Render("~/Content/font-awesome");
    @Styles.Render("~/Content/overwrite");
    <script src="https://kit.fontawesome.com/c4053d8c04.js" crossorigin="anonymous"></script>
    <link rel="stylesheet" type="text/css" href="~/Content/Home/normalize.css" />
    <link rel="stylesheet" type="text/css" href="~/Content/Home/demo.css" />
    <link rel="stylesheet" type="text/css" href="~/Content/Home/set1.css" />
    <link href="~/Content/Home/Site.css" rel="stylesheet">
    @RenderSection("Style", false)




</head>
<body>
    <nav class="navbar navbar-default navbar-fixed-top" role="navigation">
        <div class="container">
            <!-- ACOMODACION DE TAMAÑOS -->
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target=".navbar-collapse.collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="index.html">
                    <img class="logo" src="~/img/Fondo/logo2.png" alt="logo semillero">
                </a>
            </div>
            <div class="navbar-collapse collapse">
                <div class="menu">
                    <ul class="nav nav-tabs" role="tablist">
                        <li role="presentation"><a href="Index">INICIO</a></li>
                        <li role="presentation"><a href="QuienesSomos">¿QUIENES SOMOS?</a></li>
                        <li role="presentation"><a href="Trayectoria">TRAYECTORIA</a></li>
                        <li role="presentation"><a href="Actividades">ACTIVIDADES</a></li>
                        <button type="button" class="btn btn-warning boton fa fa-sign-in " onclick="location.href='login'"> Iniciar Sesion</button>
                        <button type="button" class="btn btn-warning boton fa fa-user" onclick="document.location.href='@Url.Content("~/Home/Add")'">  Registrarse</button>
                    </ul>

                </div>
            </div>
        </div>
    </nav>

    @RenderBody()


</body>

@RenderPage("~/Views/Shared/Footer.cshtml");
<!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
<script src="~/Scripts/jquery-2.1.1.min.js"></script>
<!-- Include all compiled plugins (below), or include individual files as needed -->
<script src="~/Scripts/bootstrap.min.js"></script>
<script src="~/Scripts/wow.min.js"></script>
<script src="~/Scripts/jquery.easing.1.3.js"></script>
<script src="~/Scripts/jquery.isotope.min.js"></script>
<script src="~/Scripts/jquery.bxslider.min.js"></script>
<script type="text/javascript" src="~/Scripts/fliplightbox.min.js"></script>
<script src="~/Scripts/functions.js"></script>
@RenderSection("scripts", required: false)
</html>
